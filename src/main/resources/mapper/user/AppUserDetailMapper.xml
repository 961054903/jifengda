<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caogen.jfd.dao.user.AppUserDetailDao">
	<select id="find" parameterType="AppUserDetail"
		resultType="AppUserDetail">
		SELECT
			title,description,type,money,create_date
		FROM
			app_user_detail
		WHERE 
			user_id = #{user_id}
	</select>
</mapper>