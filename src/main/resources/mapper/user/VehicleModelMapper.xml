<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caogen.jfd.dao.user.VehicleModelDao">
	<sql id="model">brand,model,length,width,height,volume,capacity,empty_cost,night_cost,jam_cost
	</sql>
	<select id="get" parameterType="VehicleModel"
		resultType="VehicleModel">
		SELECT id,
		<include refid="model" />
		FROM vehicle_model
		WHERE id = #{id}
	</select>
	<select id="find" parameterType="VehicleModel"
		resultType="VehicleModel">
		SELECT id,
		<include refid="model" />
		FROM vehicle_model
		<where>
			<if test="model != null">model = #{model}</if>
		</where>
	</select>
</mapper>