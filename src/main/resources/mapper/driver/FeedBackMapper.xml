<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caogen.jfd.dao.driver.FeedBackDao">
    <sql id="name">user_id,type,title,content,photo_url,is_check,create_date</sql>
    <insert id="insert" parameterType="FeedBack">
        INSERT
        INTO
        issue_feedback(<include refid="name"/>)
        VALUES
        (#{user_id},#{type},#{title},#{content},#{photo_url},#{is_check},#{create_date})
    </insert>
</mapper>